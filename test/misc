# TODO: not valid for pcre, same as {3,}
ignorepcre=1
1:^a{3,0}$
  m:a
  1:aaa
  1:aaaa

1:^a{3,}$
  m:a
  1:aaa
  1:aaaa

1:^a{0,3}$
  1:a
  1:aa
  1:aaa
  !:aaaa

ignorepcre=1
# pcre does not like whitespace and parse it as chars
1:^a{  1 , 2  }$
  1:a
  1:aa
  !:aaa

states=1000
trans=1000
dtimeout=10
mtimeout=0
